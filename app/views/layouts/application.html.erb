<!DOCTYPE html>
<html>
  <head>
    <title>RailsInsta</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%if logged_in? %>
      <div>
        <ul>
           <li><%= link_to "My Profile" , posts_my_post_path%></li>
          <li><%= link_to "Post Feed" , posts_post_feed_path%></li>
          <li><%= link_to "Find Friends" , users_find_path%></li>
          <li><%= link_to "New Post" , posts_new_path%></li>
        </ul>
      </div>
    <%end%>
    <%= yield %>
  </body>
</html>
