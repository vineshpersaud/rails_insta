<h1>Users#login</h1>

<div id='login'>
    <%= form_for @user , url:sessions_create_session_path do |f| %>
        <%= f.text_field :username  ,  placeholder: "Username"%><br><br>

        <%= f.password_field :password,  placeholder: "Password"%><br><br>

        <%= f.submit "Login",  id: "login_button" %>
    <%end%>
    <%= button_to "Sign Up" ,users_signup_path, id: 'sign_up_button' , :method => :get %>
</div>

