<h1>Users#login</h1>
<p>Find me in app/views/users/login.html.erb</p>
<%= form_for @user , url: users_create_session_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username%><br><br>
    
    <%= f.label :password%>
    <%= f.password_field :password%><br><br>

    <%= f.submit "Login"%>

<%end%>